{
  "name": "mycha",
  "version": "0.9.2",
  "main": "lib/mycha.js",
  "bin": {
    "mycha": "bin/mycha"
  },
  "dependencies": {
    "async": "^1.4.2",
    "coffee-script": "^1.9.0",
    "colors": "^1.0.3",
    "cross-spawn-async": "^2.0.0",
    "dargs": "^4.0.0",
    "fs-extra": "^0.23.1",
    "glob": "^5.0.3",
    "lodash": "^3.5.0",
    "minimist": "^1.1.1",
    "mocha": "^2.1.0",
    "npm": "^2.7.3",
    "prompt": "^0.2.14"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Originate/mycha.git"
  },
  "scripts": {
    "build": "find ./src -name '*.coffee' -not -name '*_spec.coffee' | xargs coffee --compile --output lib/",
    "feature-tests": "cucumber-js -f progress --strict",
    "lint": "find . -name '*.coffee' -not -path './node_modules/*' | xargs coffeelint -f coffeelint.json5",
    "postpublish": "git push && git push --tags",
    "prefeature-tests": "npm run build",
    "prepublish": "npm run build",
    "preversion": "npm test",
    "test": "npm run lint && npm run unit-tests && npm run feature-tests",
    "unit-tests": "coffee src/cli.coffee"
  },
  "author": "Alex David, Charlie Rudolph, Kevin Goslar",
  "devDependencies": {
    "blanket": "^1.1.7",
    "chai": "^1.10.0",
    "chai-fs": "^0.1.0",
    "coffeelint": "^1.10.1",
    "cucumber": "^0.4.9",
    "fixture-stdout": "^0.2.1",
    "mocha-multi": "^0.7.1",
    "sinon": "^1.14.1",
    "sinon-chai": "^2.7.0",
    "tmp": "0.0.26"
  }
}
